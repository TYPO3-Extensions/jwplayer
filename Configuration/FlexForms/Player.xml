<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<common>
			<ROOT>
				<type>array</type>
				<TCEforms>
					<sheetTitle>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.sheets.common</sheetTitle>
				</TCEforms>
				<el>
					<settings.moviesection>
						<type>array</type>
						<section>1</section>
						<el>
							<movieitem>
								<type>array</type>
								<tx_templavoila>
									<title>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.movieItem</title>
								</tx_templavoila>
								<el>
									<file>
										<TCEforms>
											<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.movie</label>
											<config>
												<type>group</type>
												<internal_type>file</internal_type>
												<allowed>mp4,mov,f4v,flv,3gp,3g2,aac,m4a</allowed>
												<max_size>50000</max_size>
												<uploadfolder>uploads/tx_jwplayer/</uploadfolder>
												<size>1</size>
												<maxitems>1</maxitems>
												<minitems>1</minitems>
											</config>
										</TCEforms>
									</file>
									<image>
										<TCEforms>
											<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.previewImage</label>
									 		<config>
												<type>group</type>
												<internal_type>file</internal_type>
												<allowed>jpg,png,gif</allowed>
												<max_size>10000</max_size>
												<uploadfolder>uploads/tx_jwplayer/</uploadfolder>
												<size>1</size>
												<maxitems>1</maxitems>
												<minitems>0</minitems>
											</config>
										</TCEforms>
									</image>
								</el>
							</movieitem>
						</el>
					</settings.moviesection>
					<settings.width>
						<TCEforms>
							<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.width</label>
							<config>
								<type>input</type>
								<eval>int</eval>
								<size>3</size>
								<default>250</default>
							</config>
						</TCEforms>
					</settings.width>
					<settings.height>
						<TCEforms>
							<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.height</label>
							<config>
								<type>input</type>
								<eval>int</eval>
								<size>3</size>
								<default>250</default>
							</config>
						</TCEforms>
					</settings.height>
					<settings.controlbar>
						<TCEforms>
							<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.controlbar</label>
							<config>
								<type>select</type>
								<maxitems>1</maxitems>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.controlbar.bottom</numIndex>
										<numIndex index="1">bottom</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.controlbar.top</numIndex>
										<numIndex index="1">top</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.controlbar.over</numIndex>
										<numIndex index="1">over</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.controlbar.none</numIndex>
										<numIndex index="1">none</numIndex>
									</numIndex>
								</items>
								<default>bottom</default>
							</config>
						</TCEforms>
					</settings.controlbar>
				</el>
			</ROOT>
		</common>
		<picture>
			<ROOT>
				<type>array</type>
				<TCEforms>
					<sheetTitle>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.sheets.picture</sheetTitle>
				</TCEforms>
				<el>
					<settings.autostart>
						<TCEforms>
							<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.autostart</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.autostart>
					<settings.repeat>
						<TCEforms>
							<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.repeat</label>
							<config>
								<type>select</type>
								<maxitems>1</maxitems>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.repeat.none</numIndex>
										<numIndex index="1">none</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.repeat.single</numIndex>
										<numIndex index="1">single</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.repeat.list</numIndex>
										<numIndex index="1">list</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.repeat.always</numIndex>
										<numIndex index="1">always</numIndex>
									</numIndex>
								</items>
								<default>none</default>
							</config>
						</TCEforms>
					</settings.repeat>
					<settings.bufferlength>
						<TCEforms>
							<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.bufferlength</label>
							<config>
								<type>input</type>
								<eval>int</eval>
								<size>3</size>
							</config>
						</TCEforms>
					</settings.bufferlength>
					<settings.stretching>
						<TCEforms>
							<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.stretching</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.stretching.none</numIndex>
										<numIndex index="1">none</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.stretching.exactfit</numIndex>
										<numIndex index="1">exactfit</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.stretching.uniform</numIndex>
										<numIndex index="1">uniform</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.stretching.fill</numIndex>
										<numIndex index="1">fill</numIndex>
									</numIndex>
								</items>
								<default>uniform</default>
							</config>
						</TCEforms>
					</settings.stretching>
				</el>
			</ROOT>
		</picture>
		<sound>
			<ROOT>
				<type>array</type>
				<TCEforms>
					<sheetTitle>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.sheets.sound</sheetTitle>
				</TCEforms>
				<el>
					<settings.volume>
						<TCEforms>
							<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.volume</label>
							<config>
								<type>input</type>
								<eval>int</eval>
								<size>2</size>
								<default>90</default>
							</config>
						</TCEforms>
					</settings.volume>
					<settings.mute>
						<TCEforms>
							<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.mute</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.mute>
				</el>
			</ROOT>
		</sound>
		<plugins>
			<ROOT>
				<type>array</type>
				<TCEforms>
					<sheetTitle>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.sheets.plugins</sheetTitle>
				</TCEforms>
				<el>
					<settings.facebookPlugin>
						<TCEforms>
							<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.facebookPlugin</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.facebookPlugin>
				</el>
			</ROOT>
		</plugins>
		<metatags>
			<ROOT>
				<type>array</type>
				<TCEforms>
					<sheetTitle>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.sheets.metatags</sheetTitle>
				</TCEforms>
				<el>
					<settings.add_metatags>
						<TCEforms>
							<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.add_metatags</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</settings.add_metatags>
					<settings.metatag_title>
						<TCEforms>
							<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.metatag_title</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
								<size>20</size>
								<max>100</max>
							</config>
						</TCEforms>
					</settings.metatag_title>
					<settings.metatag_description>
						<TCEforms>
							<label>LLL:EXT:jwplayer/Resources/Private/Language/locallang_db.xml:flexform.settings.metatag_description</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
								<size>20</size>
								<max>255</max>
							</config>
						</TCEforms>
					</settings.metatag_description>
				</el>
			</ROOT>
		</metatags>
	</sheets>
</T3DataStructure>
